{
  "version": "2017-02-28",
  "operation": "Query",
  "query": {
    "expression": "#partition = :userId and begins_with(#sort, :artistPrefix)",
    "expressionNames": {
      "#partition": "partition",
      "#sort": "sort"
    },
    "expressionValues": {
      ":userId": { "S" : "user_${ctx.identity.username}" },
      ":artistPrefix": { "S" : "artist_" }
    }
  }
}
