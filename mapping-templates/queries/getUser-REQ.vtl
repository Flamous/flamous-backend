{
  "version": "2017-02-28",
  "operation": "Query",
  "query": {
    "expression": "#partition = :userId and begins_with(#sort, :artistPrefix)",
    "expressionNames": {
      "#partition": "partition",
      "#sort": "sort"
    },
    "expressionValues": {
      ":userId": "user_${ctx.identity.username}",
      ":artistPrefix": "artist_"
    }
  }
}
